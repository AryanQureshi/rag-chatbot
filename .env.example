# Google API Key for Gemini Pro
GOOGLE_API_KEY=your_google_api_key_here

# Pinecone API Key for vector database
PINECONE_API_KEY=your_pinecone_api_key_here

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_SERVICE_ACCOUNT=your_firebase_service_account_json_here

# Gmail Configuration for Email Service
GMAIL_ADDRESS=your_gmail_address_here
GMAIL_APP_PASSWORD=your_gmail_app_password_here

# Emergency Authority Email Addresses
[emergency_authorities]
Flood = "flood.authority@example.com"
Earthquake = "earthquake.response@example.com"
Fire = "fire.department@example.com"
Medical = "medical.emergency@example.com"
General = "general.emergency@example.com"

# Note: Create a copy of this file named .env and replace the values with your actual credentials
# For security, never commit the actual .env file or API keys to version control
